array = [1,3,2,45,4,6,8,9,10]
def LinearSearch(arr,search_element):
    length = len(arr)
    for i in range(length):
        if arr[i] == search_element:
            return i
    return -1
result = LinearSearch(array,6)
if (result != -1):
    print("The number was found at index : ",result)
else:
    print("The number is not present in the array.")
    
